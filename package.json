{
  "name": "git-commits",
  "version": "1.1.0",
  "description": "Surf through Git Commits from a specific repository",
  "keywords": [
    "git",
    "reactjs"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "react": "^16.8.6",
    "react-debounce-input": "^3.2.0",
    "react-dom": "16.8.6",
    "react-scripts": "5.0.1",
    "react-select": "^2.4.3",
    "react-table": "^6.10.0",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-select": "^2.0.18",
    "@types/react-table": "^6.8.1",
    "conventional-changelog-cli": "^2.0.21",
    "istanbul-badges-readme": "^1.2.0",
    "node-sass": "^4.12.0",
    "prettier": "1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "make-badges": "istanbul-badges-readme",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
